; #####################################################################
;
; 65C22 VIA A
;
; Base address is $00D800, and registers are spaced $10 apart,
; allowing for burst mode parallel transmission using MVN/MVP
;
; #####################################################################

.include "65c22.i65"

.struct viaa
	.org $00d800
	__65c22_regs $10
.endstruct
